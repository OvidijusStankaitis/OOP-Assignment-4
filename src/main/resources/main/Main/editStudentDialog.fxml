<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns:fx="http://javafx.com/fxml" fx:controller="main.Main.EditStudentController">
    <content>
        <GridPane vgap="10" hgap="10" style="-fx-padding: 30;">
            <Label text="First name:" GridPane.rowIndex="0" GridPane.columnIndex="0" style="-fx-font-size: 16;"/>
            <TextField fx:id="firstNameField" GridPane.rowIndex="0" GridPane.columnIndex="1" style="-fx-font-size: 16;"/>
            <Label text="Last name:" GridPane.rowIndex="1" GridPane.columnIndex="0" style="-fx-font-size: 16;"/>
            <TextField fx:id="lastNameField" GridPane.rowIndex="1" GridPane.columnIndex="1" style="-fx-font-size: 16;"/>
            <Label text="Student ID:" GridPane.rowIndex="2" GridPane.columnIndex="0" style="-fx-font-size: 16;"/>
            <TextField fx:id="studentIDField" GridPane.rowIndex="2" GridPane.columnIndex="1" style="-fx-font-size: 16;"/>
        </GridPane>
    </content>
</DialogPane>
